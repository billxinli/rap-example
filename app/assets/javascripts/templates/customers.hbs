<div class="page-header">
  <h1>Customers</h1>
</div>

{{#each}}
  <div class="row">
    <h3>
      <i class="fa fa-male"></i>
      {{name}}
    </h3>
    <table width="75%" class="table table-striped table-bordered">
      <thead>
        <tr>
          <th width="10%" class="center">#</th>
          <th width="45%">Year</th>
          <th width="45%">Make</th>
        </tr>
      </thead>
      <tbody>
        {{#each vehicle in vehicles}}
          <tr>
            <td class="center">{{vehicle.id}}</td>
            <td>{{vehicle.year}}</td>
            <td>{{vehicle.make}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
{{/each}}
